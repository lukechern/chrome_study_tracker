{
  "manifest_version": 3,
  "name": "每日学习打卡",
  "version": "1.0",
  "description": "每日学习打卡插件，显示在chrome侧边栏",
  "permissions": [
    "storage",
    "tabs",
    "sidePanel"
  ],
  "action": {
    "default_title": "学习打卡",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "sidebar_7ree.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background_7ree.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["config_7ree.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
